import java.util.ArrayList;

public class Boba {

    //parameters
    private String name;
    private boolean vegan;
    private int medianPrice;
    private double distance;
    private double rating;
    private boolean milkFree;
    private boolean noCaffeine;
    private boolean hot;

    public Boba(String name, boolean vegan, int medianPrice, double distance, double rating, boolean milkFree, boolean noCaffeine, boolean hot) {
        this.name = name;
        this.vegan = vegan;
        this.medianPrice = medianPrice;
        this.distance = distance;
        this.rating = rating;
        this.milkFree = milkFree;
        this.noCaffeine = noCaffeine;
        this.hot = hot;
    }
    
    @Override
    public String toString() {
        return name;
    }


}
